  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "antfarm": "github:snarktank/antfarm",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.9.3"
  }
}
